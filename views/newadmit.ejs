<% include partials/header %>
    <div class="container blog_form">
        <h1 style="text-align : center">Add Details</h1>
        <div class="job_head"> </div>
        <form class="form-horizontal" action="/cat/<%=foundCat.title%>" method="POST">
            <div class="form-group ">
                <label>Category To Post</label>
                <select required id="dropdown_jobs" name="admitcard[category]" required>
                    <option value="">Please Select One Category</option>
                    <option value="Admit Cards">Admit Cards</option>
                    <option value="Results">Results</option>
                    <option value="Old Papers">Old Papers</option>
                </select>
            </div </div>
            <div class="form-group ">
                <label>Title Of Card</label>
                <input class="form-control" type="text" name="admitcard[title]" placeholder="title For Admit card !" required>
            </div>
            <div class="form-group">
                <label>Download Link</label>
                <input class="form-control" type="text" name="admitcard[download]" placeholder="Url Of Direct Downlaod Link" required>
            </div>
            <div class="form-group">
                <button class="btn btn-large btn-primary btn-block"> Submit !</button>
            </div>
        </form>
    </div>
    <% include partials/footer %>